<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.phonograph">

    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Phonograph"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.phonograph.callPage">
        </activity>
        <activity android:name="com.example.android.phonograph.resturauntList" />
        <activity android:name="com.example.android.phonograph.first">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.phonograph.signUp" />
        <activity android:name="com.example.android.phonograph.Sample_Restaurant" />
        <activity android:name="com.example.android.phonograph.Login" />
        <activity android:name="com.example.android.phonograph.editInformation"></activity>
    </application>

</manifest>